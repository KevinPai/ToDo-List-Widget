		<TextView android:id="@+id/note_20" android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_below="@id/note_10" android:layout_toRightOf="@id/noteimage_20"
			android:paddingLeft="3dp" android:layout_alignTop="@id/noteimage_20" />

