			android:layout_marginTop="3dp" android:layout_alignParentLeft="true" />

		<TextView android:id="@+id/note_16" android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_below="@id/note_15" android:layout_toRightOf="@id/noteimage_16"
			android:paddingLeft="3dp" android:layout_alignTop="@id/noteimage_16" />

		<ImageView android:id="@+id/noteimage_17"
			android:layout_width="20dp" android:layout_height="20dp"
			android:layout_below="@id/note_16"
			android:layout_marginTop="3dp" android:layout_alignParentLeft="true" />

		<TextView android:id="@+id/note_17" android:layout_width="wrap_content"
