			android:layout_below="@id/note_8" android:layout_toRightOf="@id/noteimage_9"
			android:paddingLeft="3dp" android:layout_alignTop="@id/noteimage_9" />

		<ImageView android:id="@+id/noteimage_10"
			android:layout_width="20dp" android:layout_height="20dp"
			android:layout_below="@id/note_9"
			android:layout_marginTop="3dp" android:layout_alignParentLeft="true" />

		<TextView android:id="@+id/note_10" android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_below="@id/note_9" android:layout_toRightOf="@id/noteimage_10"
			android:paddingLeft="3dp" android:layout_alignTop="@id/noteimage_10" />

		<ImageView android:id="@+id/noteimage_11"
			android:layout_width="20dp" android:layout_height="20dp"
